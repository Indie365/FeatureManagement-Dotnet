                {
                    "Name": "WithColor",
                    "ConfigurationValue": "https://cdn.pixabay.com/photo/2012/04/02/15/18/stegosaurus-24752_1280.png"
                },
                {
                    "Name": "HighQuality",
                    "ConfigurationValue": "https://cdn.pixabay.com/photo/2019/02/01/14/24/landscape-3969074_1280.jpg"
                }
